<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Tutotials Point">

	<test thread-count="5" name="Regression Test Suite for Chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="TutorialsPoint.PracticeTests">
				<methods>
					<include name="TC_Actions"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test thread-count="5" name="Regression Test Suite for Edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="TutorialsPoint.PracticeTests">
				<methods>
					<include name="TC_Actions"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite>